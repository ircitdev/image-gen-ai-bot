# Telegram Bot Configuration
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Stability.ai API
STABILITY_API_KEY=your_stability_api_key_here

# OpenAI API
OPENAI_API_KEY=your_openai_api_key_here

# CryptoBot API (опционально, для криптовалютных платежей)
CRYPTOBOT_TOKEN=your_cryptobot_token_here
CRYPTOBOT_CURRENCY=USDT

# Admin ID (ваш Telegram ID для админских команд)
ADMIN_ID=your_telegram_user_id_here

# Mini App Web Server URL
WEBAPP_URL=http://localhost:5000

# Google Cloud Storage (для хранения изображений)
USE_GCS=true
GCS_BUCKET_NAME=tgbots-images
GCS_CREDENTIALS_PATH=tgbots-google-cloud.json

# Google Sheets (для логирования активности)
GSHEETS_LOGGING=true
GSHEETS_SPREADSHEET_ID=your_spreadsheet_id_here
GSHEETS_CREDENTIALS_PATH=tgbots-google-sheets.json
